<form method="post" action="{{ path('app_partnership_delete', {'id_partnership': partnership.getId()}) }}" onsubmit="return confirm('Are you sure you want to delete this partnership?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ partnership.getId()) }}">
    <button class="btn btn-danger">
        <i data-feather="trash-2" class="me-1"></i>
        Delete
    </button>
</form>
